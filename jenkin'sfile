pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'checkout'
            }
        }
             stage('Build') {
            steps {
                echo 'Build'
            }
             }
             stage('Deploy') {
            steps {
                echo 'Deploying'
            }
             }
             stage('Test') {
            steps {
                echo 'Testing'
            }
             }
             stage('Release') {
            steps {
                echo 'Release'
             }  
                 
             }
 }

}
